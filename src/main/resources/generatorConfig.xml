<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<properties resource="conf/jdbc.properties" />
	<context id="context1">
		<jdbcConnection connectionURL="${jdbc.url}"
			driverClass="${jdbcDriver}" password="${jdbcPassword}" userId="${jdbcUsername}" />
		<javaModelGenerator targetPackage="com.huxm.demo.demo1.entity.auto"
			targetProject="src/main/java" />
		<sqlMapGenerator targetPackage="mybatis-mapper-auto"
			targetProject="src/main/resources" />
		<javaClientGenerator targetPackage="com.huxm.demo.demo1.dao.auto"
			targetProject="src/main/java" type="XMLMAPPER" />
		<table schema="S1" tableName="USER">
			<!--columnOverride column="???" property="???" / -->
		</table>
		<table schema="S1" tableName="REQUEST">
			<!--columnOverride column="???" property="???" / -->
		</table>
	</context>
</generatorConfiguration>